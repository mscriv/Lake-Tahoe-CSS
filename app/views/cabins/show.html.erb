<p id="notice"><%= notice %></p>

<body>

<h1 id="H1_1">
  <%= @cabin.title %>
</h1>
<div class="photo">
<%= image_tag @cabin.image_url(:thumb).to_s %>
</div>
<p>
  <strong>From</strong>
  <h4>$<%= @cabin.price %> per night</h4>
</p>

<div id="DIV_1">
  <ul class= UL">
    <li id="LI_3"><%= @cabin.bedrooms %> Bedrooms</li>
    <li id="LI_4"><%= @cabin.bathrooms %> Bathrooms</li>
    <li id="LI_5">Sleeps <%= @cabin.sleeps %></li>
    <li id="LI_6"><%= @cabin.type %></li>


    <h4>Description from the Manager</h4>
      <p>
        <%= @cabin.overview %>
      </p>
</div>

<h3>Rates</h3>
<div id="DIV_1">
<table summary="Rates" id="TABLE_2">
  <thead id="THEAD_3">
  <tr id="TR_4">
    <th id="TH_5">Dates</th>
    <th id="TH_6">Nightly</th>
    <th id="TH_7">Weekly</th>
    <th id="TH_9">Monthly</th>
    <th id="TH_9">Minimum Stay</th>

  </tr>
  </thead>

<tbody id="TBODY_10">
  <% @cabin.rates.each do |rate| %>
	<tr id="TR_11">
	  <td colspan="5" id="TD_12">
	    <table id="TABLE_13">
	      <tbody id="TBODY_14">
	      <tr id="TR_15">
	        <th id="TH_16">
	  <div id="DIV_17"><%= rate.date %></div>
	          <div id="DIV_18"></div>
	          <div id="DIV_19"></div>
	  <td id="TD_20">$<%= rate.nightly %></td>
	  <td id="TD_21">$<%= rate.weekly %></td>
	        <td id="TD_22">Not Listed</td>
	  <td id="TD_23"><%= rate.minimum_stay %></td>
	</tr>
	<% end %>
</tbody>
</table>

<button id="BUTTON_189">
<%= link_to 'Back', cabins_path %>
</button>

</body>